{{#each this}}
<div>
    <h2 class="countrieName">{{name}}</h2>
    <p>Capital: {{capital}}</p>
    <p>Population: {{population}}</p>
    <p> Languages:
    <ul>
        {{#each this.languages}}
        <li>{{name}}</li>
        {{/each}}
    </ul>
</div>
<img src="{{flag}}" alt="flag" class="js-flag-countrie">
{{/each}}